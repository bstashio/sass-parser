{
  "name": "sass-parser",
  "version": "0.1.2",
  "description": "A simple npm package to parse a Sass file into a consumable json data",
  "main": "index.js",
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "preversion": "npm test && npm run docs:api",
    "version": "git add --all",
    "postversion": "git push && git push --tags",
    "docs:api": "documentation readme index.js --section=API"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bstashio/sass-parser.git"
  },
  "keywords": [
    "sass",
    "parse",
    "parser",
    "variables",
    "mixins",
    "functions",
    "json"
  ],
  "author": {
    "name": "bstashio",
    "email": "bstashio@gmx.com",
    "url": "http://bstash.io"
  },
  "license": "MIT",
  "homepage": "https://github.com/bstashio/sass-parser",
  "bugs": {
    "url": "https://github.com/bstashio/sass-parser/issues"
  },
  "engines": {
    "node": ">=6"
  },
  "files": [
    "index.js",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "line-by-line": "0.1.5",
    "lodash": "4.17.4"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "coveralls": "3.0.0",
    "documentation": "5.3.4",
    "mocha": "4.0.1",
    "nyc": "11.4.1"
  }
}
